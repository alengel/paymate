<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule> 
        <from-view-id>/login.xhtml</from-view-id> 
        <navigation-case> 
            <from-outcome>registration</from-outcome> 
            <to-view-id>/registration.xhtml</to-view-id> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>user</from-outcome> 
            <to-view-id>/user/notifications.xhtml</to-view-id>
            <redirect></redirect> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>admin</from-outcome> 
            <to-view-id>/admin/accounts.xhtml</to-view-id>
            <redirect></redirect> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>error</from-outcome> 
            <to-view-id>/login_failure.xhtml</to-view-id> 
        </navigation-case>  
    </navigation-rule>
    
    <navigation-rule> 
        <from-view-id>/registration.xhtml</from-view-id> 
        <navigation-case> 
            <from-outcome>success</from-outcome> 
            <to-view-id>/registration_success.xhtml</to-view-id> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>login</from-outcome> 
            <to-view-id>/login.xhtml</to-view-id> 
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule> 
        <from-view-id>/registration_success.xhtml</from-view-id> 
        <navigation-case> 
            <from-outcome>login</from-outcome> 
            <to-view-id>/login.xhtml</to-view-id> 
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule> 
        <from-view-id>/user/notifications.xhtml</from-view-id> 
        <navigation-case> 
            <from-outcome>login</from-outcome> 
            <to-view-id>/login.xhtml</to-view-id>
            <redirect></redirect> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>notifications</from-outcome> 
            <to-view-id>/user/notifications.xhtml</to-view-id> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>payments</from-outcome> 
            <to-view-id>/user/payments.xhtml</to-view-id> 
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule> 
        <from-view-id>/user/payments.xhtml</from-view-id> 
        <navigation-case> 
            <from-outcome>login</from-outcome> 
            <to-view-id>/login.xhtml</to-view-id>
            <redirect></redirect> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>notifications</from-outcome> 
            <to-view-id>/user/notifications.xhtml</to-view-id> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>payment_success</from-outcome> 
            <to-view-id>/user/payments_success.xhtml</to-view-id> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>request_success</from-outcome> 
            <to-view-id>/user/request_success.xhtml</to-view-id> 
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule> 
        <from-view-id>/user/payments_success.xhtml</from-view-id> 
        <navigation-case> 
            <from-outcome>login</from-outcome> 
            <to-view-id>/login.xhtml</to-view-id>
            <redirect></redirect> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>notifications</from-outcome> 
            <to-view-id>/user/notifications.xhtml</to-view-id> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>payments</from-outcome> 
            <to-view-id>/user/payments.xhtml</to-view-id> 
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule> 
        <from-view-id>/user/payments_failure.xhtml</from-view-id> 
        <navigation-case> 
            <from-outcome>login</from-outcome> 
            <to-view-id>/login.xhtml</to-view-id>
            <redirect></redirect> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>notifications</from-outcome> 
            <to-view-id>/user/notifications.xhtml</to-view-id> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>payments</from-outcome> 
            <to-view-id>/user/payments.xhtml</to-view-id> 
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule> 
        <from-view-id>/user/request_success.xhtml</from-view-id> 
        <navigation-case> 
            <from-outcome>login</from-outcome> 
            <to-view-id>/login.xhtml</to-view-id>
            <redirect></redirect> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>notifications</from-outcome> 
            <to-view-id>/user/notifications.xhtml</to-view-id> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>payments</from-outcome> 
            <to-view-id>/user/payments.xhtml</to-view-id> 
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule> 
        <from-view-id>/admin/accounts.xhtml</from-view-id> 
        <navigation-case> 
            <from-outcome>login</from-outcome> 
            <to-view-id>/login.xhtml</to-view-id>
            <redirect></redirect> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>accounts</from-outcome> 
            <to-view-id>/admin/accounts.xhtml</to-view-id>
            <redirect></redirect> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>transactions</from-outcome> 
            <to-view-id>/admin/transactions.xhtml</to-view-id>
            <redirect></redirect> 
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule> 
        <from-view-id>/admin/transactions.xhtml</from-view-id> 
        <navigation-case> 
            <from-outcome>login</from-outcome> 
            <to-view-id>/login.xhtml</to-view-id>
            <redirect></redirect> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>accounts</from-outcome> 
            <to-view-id>/admin/accounts.xhtml</to-view-id>
            <redirect></redirect> 
        </navigation-case>
        <navigation-case> 
            <from-outcome>transactions</from-outcome> 
            <to-view-id>/admin/transactions.xhtml</to-view-id>
            <redirect></redirect> 
        </navigation-case>
    </navigation-rule>
    
</faces-config>
